local GlobalGrid = Grid

---@class ge_tts__Grid : tts__GlobalGrid
local Grid = {}

Grid.Type = {
    Box = 1,
    HexHorizontal = 2,
    HexVertical = 3,
}

Grid.Snapping = {
    None = 1,
    Lines = 2,
    Center = 3,
    LinesAndCenter = 4,
}

setmetatable(Grid, {
    __index = GlobalGrid,
    __newindex = function(_, key, value)
        GlobalGrid[key] = value
    end
})

return Grid
